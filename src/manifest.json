{
    "name": "Name instead of UserId for SAP Cloud Cockpit",
    "description": "Display full name instead of UserId for SAP Cloud Platform Cockpit",
    "version": "0.1",
    "manifest_version": 2,
    "content_scripts": [{
        "js": [
            "index.bundle.js"
        ],
        "matches": [
            "https://account.hana.ondemand.com/cockpit*"
        ]
    }],
    "permissions": [
    	"storage"
    ],
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    }
}
