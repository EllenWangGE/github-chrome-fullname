{
    "name": "Name instead of UserId for SAP GitHub",
    "description": "Display full name instead of UserId for SAP GitHub Enterprise instance",
    "version": "0.3.9",
    "manifest_version": 2,
    "content_scripts": [{
        "js": [
            "index.bundle.js"
        ],
        "matches": [
            "*://github.wdf.sap.corp/*",
            "*://huboard.mo.sap.corp/*"
        ],
        "exclude_matches": [
        	"https://github.wdf.sap.corp/pages*"
        ]
    }],
    "permissions": [
    	"storage"
    ],
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    }
}
